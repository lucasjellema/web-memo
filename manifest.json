{
  "manifest_version": 3,
  "name": "Web Memo",
  "description": "A Chrome Extension to bundle Web Pages, Links, Images and Notes in hierarchically organized projects. Similar to bookmarks - with more ease and control.",
  "version": "0.6",
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting",
    "sidePanel"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "web-memo-content.js"
      ],
      "run_at": "document_end"
    }, {
        "matches": ["https://www.google.com/maps/*"],
        "js": ["google-maps-content.js"]
      }
    
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "side_panel": {
    "default_path": "web_memo_side_panel.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}